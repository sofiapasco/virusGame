generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  nickname String
  scores   Int[]
}

model Room {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name  String
}